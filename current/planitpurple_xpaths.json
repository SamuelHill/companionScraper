{
	"page": "https://planitpurple.northwestern.edu",
	"loop": {
		"events_list": "//ul[@class=\"events\"]",
		"attributes": [
			{
				"label": "name",
				"xpath": "//div[@class=\"event_header\"]/h2/text()",
			},
			{
				"label": "month",
				"xpath": "//div[@class=\"event_date\"]/div[@class=\"month\"]/text()",
			},
			{
				"label": "day",
				"xpath": "//div[@class=\"event_date\"]/div[@class=\"day\"]/text()",
			},
			{
				"label": "dayofweek",
				"xpath": "//div[@class=\"event_date\"]/div[@class=\"time\"]/text()",
			},
		]
	},
}

